---
import SingleArticle from "../../components/SingleArticle.astro"
import { listingList } from "../../queries/listings.js"

import Header from "../../components/Header.astro"
---

<Header>

</Header>

<h1>Listing</h1>

<div style="display: flex;">
{
    listingList.posts.data.map((item) => (
        <SingleArticle url={item.attributes.slug} image={item.attributes.image} name={item.attributes.name} />
    ))
}
</div>